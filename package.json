{
  "name": "hurry",
  "productName": "hurry",
  "version": "0.1.5",
  "description": "Daily Helpdesk / admin's tool",
  "main": ".webpack/main",
  "scripts": {
    "startroot": "electron-forge start -- --no-sandbox",
    "start": "electron-forge start",
    "buildplugins": "node watchplugins.js",
    "releaseplugins": "node releaseplugins.js",
    "test": "mocha --require @babel/register --recursive",
    "package": "env NODE_ENV=production electron-forge package",
    "make": "env NODE_ENV=production electron-forge make",
    "make-windows": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "rebrec",
    "github": "https://github.com/rebrec"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Hurry"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux",
            "win32"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/ui/index.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@babel/node": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/webpack-env": "^1.16.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-mobx": "^2.0.0",
    "bluebird": "^3.7.2",
    "bootstrap": "^4.4.0",
    "buffer-builder": "^0.2.0",
    "commander": "^6.1.0",
    "custom-electron-titlebar": "^3.1.0",
    "electron-react-devtools": "^0.5.3",
    "electron-squirrel-startup": "^1.0.0",
    "fifo": "^2.3.0",
    "generic-pool": "2.4.4",
    "hurry-plugin-definition": "^1.0.2",
    "jquery": "^3.4.1",
    "lodash": "^4.17.20",
    "mobx": "^5.15.0",
    "mobx-react": "^6.1.4",
    "mocha": "^6.2.2",
    "net-ping": "^1.2.3",
    "node-powershell": "^4.0.0",
    "node-sass": "^4.13.0",
    "ping": "^0.2.2",
    "popper.js": "^1.16.0",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.12.0",
    "react-jsonschema-form": "^1.8.1",
    "react-split-pane": "^0.1.89",
    "sass-loader": "^8.0.0",
    "targz": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-react": "^7.7.4",
    "@electron-forge/cli": "6.0.0-beta.45",
    "@electron-forge/maker-deb": "6.0.0-beta.45",
    "@electron-forge/maker-rpm": "6.0.0-beta.45",
    "@electron-forge/maker-squirrel": "6.0.0-beta.45",
    "@electron-forge/maker-zip": "6.0.0-beta.45",
    "@electron-forge/plugin-webpack": "6.0.0-beta.45",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/chai": "^4.2.15",
    "@types/jest": "^24.0.23",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.0.6",
    "chai": "^4.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "css-loader": "^3.0.0",
    "electron": "7.1.1",
    "electron-rebuild": "^1.10.1",
    "file-loader": "^6.1.0",
    "fork-ts-checker-webpack-plugin": "^3.1.0",
    "jest": "^26.6.3",
    "node-loader": "^0.6.0",
    "prettier": "^1.19.1",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^3.7.2",
    "webpack-cli": "^3.3.12"
  }
}
